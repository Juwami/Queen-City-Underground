<div class="container">
    <div class="wrapper">
        <div id="myCarousel" class="carousel carousel-fade slide container" data-ride="carousel" data-interval="7000">
            <div class="carousel-inner">
                <div class="active carousel-item one">
                    <img class="mx-auto" src="/images/QCU_backgrounds/reason_define.jpg">
                </div>
                <div class="carousel-item two">
                    <img class="mx-auto" src="/images/QCU_backgrounds/oso_ck.jpg">
                </div>
                <div class="carousel-item three">
                    <img class="mx-auto" src="/images/QCU_backgrounds/never_i_amos.jpg">
                </div>
                <div class="carousel-item four">
                    <img class="mx-auto" src="/images/QCU_backgrounds/oso_ck2.jpg">
                </div>
                <div class="carousel-item five">
                    <img class="mx-auto" src="/images/QCU_backgrounds/reflect_refine_oso.jpg">
                </div>
            </div>
        </div>
    </div>
    <a href="/login" class="text-white float-right">PROMOTER LOGIN</a>
    <div class="row">
        <div class="col col-12 text-center m-5 float">

            <header>
                <div id="logo">
                    <h1>QUEEN CITY</h1>
                    <h3>Underground</h3>
                </div>
            </header>

        </div>
    </div>
    <div class="row">

        <div class="col col-12">
            <form>
                <div class="text-center" style="width: 245px;">
                    <div class="input-group mb-2 text-center">
                        <div class="input-group-prepend">
                            <span class="input-group-text">Start</span>
                        </div>
                        <input id="start-date" type="date" name="startDate">

                    </div>
                    <div class="input-group mb-2">
                        <div class="input-group-prepend">
                            <span class="input-group-text">End</span>
                        </div>
                        <input id="end-date" type="date" name="endDate">

                    </div>



                    <button id="submit-btn" class="btn btn-lg btn-primary m-5" type="submit">Submit</button>
                </div>
            </form>

        </div>
        <div class="col col-12 p-0">
            <div class="venueData">
                <table class="table table-secondary table-hover">
                    <thead>
                        <tr>

                            <th>Date</th>
                            <th>Event</th>
                            <th>Venue</th>
                        </tr>
                    </thead>
                    <tbody>
                        {{#each events}}
                        <tr id={{this.id}} class="js_row">
                            <td>{{this.eventDateTime}}</td>
                            <td>{{this.eventName}}</td>
                            <td>{{this.Venue.venueName}}</td>
                        </tr>
                        {{/each}}
                    </tbody>
                </table>
                <div>
                </div>
            </div>

            <!-- Modal -->
            <div class="modal fade" id="event-modal" tabindex="-1" role="dialog" aria-labelledby="exampleModal3Label"
                aria-hidden="true">
                <div class="modal-dialog" role="document">
                    <div class="modal-content">
                        <div class="modal-header">
                            <h5 class="modal-title" id="event-title">Event</h5>
                            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                <span aria-hidden="true">&times;</span>
                            </button>
                        </div>
                        <div class="modal-body">
                                                        
                        </div>
                        <div class="modal-footer">
                            <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>                   
                        </div>
                    </div>
                </div>
            </div>



        </div>

        <script src="js/index.js"></script>